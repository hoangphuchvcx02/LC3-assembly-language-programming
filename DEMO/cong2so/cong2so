		.ORIG X3000
IN 
LD R1, ASCII_0

NOT R1, R1
ADD R2, R1, #1
ADD R2, R0, R1

IN 
ADD R2, R0, R2
ADD R2, R2, R1
ADD R3, R2, #-10

BRn x7

LD R0, xA
ADD R0, R0, #1
OUT
LD R0, x7
ADD R0, R0, R3
OUT

BR x3
LD R0, x3
ADD R0, R0, R3
OUT

BRx3
LD R0, x3
ADD R0, R0, R2
OUT
HALT 
	ASCII_0	.FILL x-48
		
		.END